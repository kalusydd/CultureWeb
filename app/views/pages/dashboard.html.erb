<div class="dashboard-background">

<div data-controller="headline" data-headline-bookings-value="My bookings">
    <h1 class="header-all"></h1>
</div>


<div class="container-booking">
  <% if current_user.bookings.exists? %>
    <%= render "bookings_as_attendee" %>
  <% else %>
    <div class= "not">
        <body-all > Give a fuck (no attendance) </body-all>
        </div>
        <%= link_to "Check out cool events near you", root_path, class:"btn btn-ghost-orange-more" %>
      <% end %>
</div>


<div class="my-events">

<div data-controller="headline" data-headline-events-value="My events">
    <h1 class="header-all"></h1>
</div>

<div class="container-event">
  <% if current_user.events.exists? %>
    <%= render "events_as_org" %>
  <% else %>
    <div class= "not-two">
        <body-all > You lazy fuck (no events) </body-all>
        </div>
    <%= link_to "New event, new life ", new_event_path, class:"btn btn-ghost-orange-new_event" %>
  <% end %>
</div>
</div>
</div>










</div>
