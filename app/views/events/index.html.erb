<h1>All events</h1>
<div class="card p-2 m-3">
  <% @events.each do |event| %>
    <li>
      <h2><%= event.title %></h2>
      <p>Date: <%= event.date %></p>
      <p>Time: <%= event.time %></p>
      <p>Description: <%= event.description %></p>
      <p>Venue: <%= event.venue_name %></p>
      <p>Address: <%= event.venue_address %></p>
      <p>Capacity: <%= event.capacity %></p>
      <p>Price: <%= event.price %></p>
    </li>
  <% end %>
</div>
